{
  "name": "node-mysql2-wrapper",
  "version": "0.0.0",
  "description": "Shortcuts and utilities for the node-mysql2 package",
  "main": "dist/index.js",
  "dependencies": {
    "es6-promise": "^3.1.2",
    "mysql2": "^1.0.0-rc.1",
    "node-uuid": "^1.4.7",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "tsc": "^1.20150623.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "uncommitted": "^1.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run typings install && npm run tsc",
    "predopublish": "npm run build && uncommitted",
